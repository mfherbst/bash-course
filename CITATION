  author       = {Michael F. Herbst},
  title        = {Advanced bash scripting 2017},
  month        = nov,
  year         = 2017,
  doi          = {10.5281/zenodo.1045332},
  url          = {https://doi.org/10.5281/zenodo.1045332}
}
